#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		int n = 4;
		vector<int> arr(n);
		for (int i = 0; i < n; i++)
			cin >> arr[i];
		sort(arr.begin(), arr.end());
		if (arr[0] == arr[1] && arr[2] == arr[3])
			cout << "YES" << endl;
		else
			cout << "NO" << endl;
	}
}
